{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Pico Debug (Container)",
      "cwd": "${workspaceRoot}",
      "executable": "${workspaceRoot}/build/kaluma-picocalc-pico-1.2.1.elf",
      "request": "launch",
      "type": "cortex-debug",
      "servertype": "openocd",
      "serverpath": "/workspaces/kaluma-project/.devcontainer/sudo_openocd.sh",
      "gdbPath": "gdb-multiarch",
      "device": "RP2040",
      "configFiles": [
        "interface/cmsis-dap.cfg",
        "target/rp2040.cfg"
      ],
      "openOCDLaunchCommands": [
        "adapter speed 5000",
      ],
      "svdFile": "${workspaceRoot}/lib/pico-sdk/src/rp2040/hardware_regs/rp2040.svd",
      "runToEntryPoint": "main",
      "showDevDebugOutput": "parsed",
      "preLaunchTask": "node build",
      "postRestartCommands": [
        "break main",
        "continue"
      ]
    }
